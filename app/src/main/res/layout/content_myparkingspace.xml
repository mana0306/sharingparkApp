<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.lzj.its.sharingpark.activity.ParkingspaceActivity"
    tools:showIn="@layout/activity_myparkingspace">
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:stv="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/ct_account"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.lzj.its.sharingpark.fragment.AccountFragment"
        android:orientation="vertical"
        android:paddingTop="20dp">
        <com.allen.library.SuperTextView
            android:id="@+id/stv_parkinglot"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            stv:sCenterTextString=""
            stv:sCenterViewGravity="left_center"
            stv:sLeftTextString="停车场："
            stv:sLeftViewGravity="left_center"
            stv:sLeftViewWidth="80dp" />

        <com.allen.library.SuperTextView
            android:id="@+id/stv_num"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            stv:sCenterTextString=""
            stv:sCenterViewGravity="left_center"
            stv:sLeftTextString="车位号："
            stv:sLeftViewGravity="left_center"
            stv:sLeftViewWidth="80dp" />

        <com.allen.library.SuperTextView
            android:id="@+id/stv_address"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            stv:sCenterTextString=""
            stv:sCenterViewGravity="left_center"
            stv:sLeftTextString="详细地址："
            stv:sLeftViewGravity="left_center"
            stv:sLeftViewWidth="80dp" />

        <com.allen.library.SuperTextView
            android:id="@+id/stv_more"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            stv:sCenterTextString=""
            stv:sCenterViewGravity="left_center"
            stv:sLeftTextString="其他："
            stv:sLeftViewGravity="left_center"
            stv:sLeftViewWidth="80dp" />
    </LinearLayout>


</android.support.constraint.ConstraintLayout>
